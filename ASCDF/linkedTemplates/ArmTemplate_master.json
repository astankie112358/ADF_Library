{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ASCDF"
		},
		"StgBlobParquet_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StgBlobParquet'"
		},
		"StorageSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StorageSource'"
		},
		"AzureDBOConnection_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "asctest123.database.windows.net"
		},
		"AzureDBOConnection_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "LibrarySQL"
		},
		"AzureDBOConnection_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "app_user"
		},
		"AzureLibraryKV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://LibraryLogins.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ASCDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"StgBlobParquet_connectionString": {
						"value": "[parameters('StgBlobParquet_connectionString')]"
					},
					"StorageSource_connectionString": {
						"value": "[parameters('StorageSource_connectionString')]"
					},
					"AzureLibraryKV_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureLibraryKV_properties_typeProperties_baseUrl')]"
					},
					"AzureDBOConnection_properties_typeProperties_server": {
						"value": "[parameters('AzureDBOConnection_properties_typeProperties_server')]"
					},
					"AzureDBOConnection_properties_typeProperties_database": {
						"value": "[parameters('AzureDBOConnection_properties_typeProperties_database')]"
					},
					"AzureDBOConnection_properties_typeProperties_userName": {
						"value": "[parameters('AzureDBOConnection_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}